class Solution {
    public boolean isPalindrome(int x) {
        int sum =0;
        int n =x;
        while(x>0){
            int ans = x%10;
            x= x/10;
            sum=sum*10 + ans;
        }
        if(sum == n ){
            return true;
            
        }else 
            return false;
        
    }
}
